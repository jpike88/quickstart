// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.js": true,
        "**/*.js.map": true,
        ".vscode": true,
        "**/*.json": true,
        "**/*.eot": true,
        "**/*.svg": true,
        "**/*.ttf": true,
        "**/*.woff": true,
        "**/api.ts": true
    },
    "search.exclude": {
        "**/api.ts": true,
        "**/*.png": true,
        "**/*.js": true,
        "**/*.js.map": true,
        "**/*.jpg": true,
        "**/*.json": true,
        "**/*.css": true,
        "**/*.eot": true,
        "**/*.svg": true,
        "**/*.ttf": true,
        "**/*.woff": true
    },
    "editor.fontSize": 14,
    "editor.snippetSuggestions": "top",
    "editor.autoIndent": true
}